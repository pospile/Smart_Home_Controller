<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="cz.pospichal.vojtech" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <icon height="62" src="res/icons/wp8/ApplicationIcon.png" width="62" />
    <icon height="173" src="res/icons/wp8/Background.png" width="173" />
    <splash height="800" src="res/screens/wp8/SplashScreenImage.jpg" width="480" />
    <name>Chytrá Domácnost</name>
    <description>Smart Home Controller App</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Vojtěch Pospíchal</author>
    <content src="index.html" />
    <access origin="*" />
    <vs:features />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="Fullscreen" value="True" />
    <preference name="KeepRunning" value="False" />
    <preference name="ShowTitle" value="True" />
    <preference name="InAppBrowserStorageEnabled" value="False" />
    <preference name="SuppressesIncrementalRendering" value="True" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.12" />
    <vs:plugin name="com.pushbots.push" />
</widget>
